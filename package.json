{
  "name": "vite-plugin-svg-icons",
  "version": "0.1.1",
  "description": "Vite Plugin for fast creating SVG sprites.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Vben",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "prepublishOnly": "yarn build",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -r",
    "example:dev": "npm -C example run serve",
    "example:build": "npm -C example run build"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "svg",
    "sprite",
    "svgo",
    "vben"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anncwb/vite-plugin-svg-icons"
  },
  "bugs": {
    "url": "https://github.com/anncwb/vite-plugin-svg-icons/issues"
  },
  "homepage": "https://github.com/anncwb/vite-plugin-svg-icons/tree/master/#readme",
  "dependencies": {
    "debug": "^4.3.2",
    "etag": "^1.8.1",
    "fs-extra": "^9.1.0",
    "svg-baker": "1.7.0",
    "svgo": "^2.2.0"
  },
  "peerDependencies": {
    "vite": ">=2.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/etag": "^1.8.0",
    "@types/fs-extra": "^9.0.8",
    "@types/node": "^14.14.31",
    "@types/svgo": "^1.3.4",
    "conventional-changelog-cli": "^2.1.1",
    "ini": "^2.0.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "tsup": "^4.6.1",
    "typescript": "^4.2.3",
    "vite": "^2.0.5"
  }
}
